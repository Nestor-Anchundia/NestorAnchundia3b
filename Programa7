using System;

namespace Programa7
{
    class Program
    {
        static void Main(string[] args)
        {
            int  a=0,b=101;
            for (int i = 1; i <= 100 ; i++)
            {
                Console.WriteLine("{0}<<<>>>>{1}", a = a + 1, b = b - 1);
            }
            Console.ReadLine();
        }
    }
}
